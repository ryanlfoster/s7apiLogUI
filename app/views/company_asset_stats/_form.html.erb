<%= form_for(@db_call) do |f| %>
  <% if @db_call.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@db_call.errors.count, "error") %> prohibited this db_call from being saved:</h2>

      <ul>
      <% @db_call.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :definedVars %><br />
    <%= f.text_field :definedVars %>
  </div>
  <div class="field">
    <%= f.label :varsToShow %><br />
    <%= f.text_field :varsToShow %>
  </div>
  <div class="field">
    <%= f.label :varsToGroup %><br />
    <%= f.text_area :varsToGroup %>
  </div>
  <div class="field">
    <%= f.label :startDate %><br />
    <%= f.date_select :startDate %>
  </div>
  <div class="field">
    <%= f.label :endDate %><br />
    <%= f.date_select :endDate %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
